{
  "name": "d3-zoomable",
  "version": "1.4.0",
  "description": "Easy way to apply d3-zoom functionality to DOM elements",
  "license": "MIT",
  "type": "module",
  "jsdelivr": "dist/d3-zoomable.min.js",
  "unpkg": "dist/d3-zoomable.min.js",
  "main": "dist/d3-zoomable.mjs",
  "module": "dist/d3-zoomable.mjs",
  "exports": {
    "umd": "./dist/d3-zoomable.min.js",
    "default": "./dist/d3-zoomable.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/d3-zoomable.git"
  },
  "homepage": "https://github.com/vasturiano/d3-zoomable",
  "keywords": [
    "zooming",
    "svg",
    "canvas",
    "d3",
    "data-visualization"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://github.com/vasturiano"
  },
  "bugs": {
    "url": "https://github.com/vasturiano/d3-zoomable/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -w -c",
    "prepare": "npm run build"
  },
  "dependencies": {
    "d3-interpolate": "1 - 3",
    "d3-selection": "2 - 3",
    "d3-transition": "2 - 3",
    "d3-zoom": "2 - 3",
    "kapsule": "^1.16"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.28.0"
  },
  "engines": {
    "node": ">=12"
  }
}
